# SSO (required for JWT validation)
SSO_URL=http://localhost:3001

# GitHub content source
GITHUB_REPO=panaversity/agentfactory
GITHUB_TOKEN=

# Redis (optional - caching degrades gracefully without it)
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=

# Progress API (optional - completion tracking)
PROGRESS_API_URL=http://localhost:8002

# Token Metering (optional)
METERING_API_URL=
METERING_ENABLED=false

# CORS
ALLOWED_ORIGINS=http://localhost:3000

# Dev mode - bypasses auth for local development
DEV_MODE=false

# Debug
LOG_LEVEL=INFO
PORT=8003
